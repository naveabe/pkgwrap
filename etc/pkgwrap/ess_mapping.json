{
    "_default_": {
        "dynamic_templates": [
            {
                "timestamp": {
                    "match"             : "timestamp",
                    "mapping"           : {"type": "float"}
                }
            },{
                "tagbranch": {
                    "match"             : "tagbranch",
                    "mapping"           : {"type": "string", "index": "not_analyzed"}
                }
            },{
                "version": {
                    "match"             : "version",
                    "mapping"           : {"type": "string", "index": "not_analyzed"}
                }
            },{
                "jobs": {
                    "match": "jobs",
                    "mapping": {"type": "object"},
                    "properties": {
                        "id": {"type":"string"},
                        "uri": {"type": "string", "index": "not_analyzed"}
                    }
                }
            }
        ]
    }
}